{
  "name": "kehadiran",
  "plural": "kehadiran",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "datePattern": "dd-MM-yyy",
    "validateUpsert": true
  },
  "postgresql": {
    "schema": "public",
    "table": "kehadiran"
  },
  "properties": {
    "id_pengguna": {
      "type": "number",
      "required": false
    },
    "nama_pengguna": {
      "type": "string",
      "required": false
    },
    "tanggal": {
      "type": "string",
      "required": true,
      "format": "date"
    },
    "waktu_masuk": {
      "type": "string",
      "required": false
    },
    "waktu_keluar": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "pengguna": {
      "type": "belongsTo",
      "model": "pengguna",
      "foreignKey": "id_pengguna",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "*"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": "*"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": [
        "find", "masuk", "keluar"
      ]
    }
  ],
  "methods": {}
}
