    "email": {
      "type": "string",
      "required": true
    },
    "password": {
      "type": "String",
      "required": true,
      "max": 50,
      "min": 8,
      "pattern": "(?=.*[A-Z])(?=.*[!@#$&*])(?=.*[0-9])(?=.*[a-z])^.*$",
      "default": "ayaya"
    },
    "role": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
