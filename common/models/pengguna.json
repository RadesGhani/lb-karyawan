{
  "name": "pengguna",
  "plural": "pengguna",
  "base": "User",
  "idInjection": true,
  "restrictResetPasswordTokenScope": true,
  "emailVerificationRequired": false,
  "postgresql": {
    "schema": "public",
    "table": "pengguna"
  },
  "properties": {
    "username": {
      "type": "String",
      "required": true,
      "max": 50,
      "min": 2
    },
    "email": {
      "type": "string",
      "required": true
    },
    "password": {
      "type": "String",
      "required": true,
      "max": 50,
      "min": 8,
      "pattern": "(?=.*[A-Z])(?=.*[!@#$&*])(?=.*[0-9])(?=.*[a-z])^.*$",
      "default": "ayaya"
    },
    "role": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "cuti_input": {
      "type": "hasMany",
      "model": "cuti_input",
      "foreignKey": "id_pengguna",
      "options": {
        "nestRemoting": true
      }
    },
    "kehadiran": {
      "type": "hasMany",
      "model": "kehadiran",
      "foreignKey": "id_pengguna",
      "options": {
        "nestRemoting": true
      }
    },
    "tim": {
      "type": "belongsTo",
      "model": "tim",
      "foreignKey": "team_name_id",
      "options": {
        "nestRemoting": true
      }
    },
    "profil": {
      "type": "hasOne",
      "model": "profil",
      "foreignKey": "id_pengguna",
      "options": {
        "nestRemoting": true
      }
    },
    "gaji": {
      "type": "hasOne",
      "model": "gaji      ",
      "foreignKey": "id_pengguna",
      "options": {
        "nestRemoting": true
      }
    },
    "pinjaman": {
      "type": "hasMany",
      "model": "pinjaman",
      "foreignKey": "id_pengguna",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType":"READ",
      "principalType":"ROLE",
      "principalId":"$everyone",
      "property":"*",
      "permission":"ALLOW"
    },
    {
      "accessType":"READ",
      "principalType":"ROLE",
      "principalId":"$authenticated",
      "property":"*",
      "permission":"ALLOW"
    },
    {"accessType":"EXECUTE",
      "principalType":"ROLE",
      "principalId":"$everyone",
      "property":[
        "create","deleteById","patchAttributes",
        "__delete__cuti","__create__cuti", "__destroyById__cuti","__updateById__cuti",
        "__delete__kehadiran","__create__kehadiran", "__destroyById__kehadiran","__updateById__kehadiran","__get__kehadiran",
        "__get__profil","__delete__profil","__create__profil", "__destroyById__profil","__updateById__profil", "__create__team_project__task", "__get__team_project", "__get__task"
      ],
      "permission":"ALLOW"
    },
    {
      "accessType":"EXECUTE",
      "principalType":"ROLE",
      "principalId":"admin",
      "property":[
        "create","deleteById","patchAttributes",
        "__create__cuti", "__destroyById__cuti", "__updateById__cuti",
        "__get__kehadiran",
        "__get__profil", "__create__profil", "__destroy__profil","__updateById__profil", "__create__team_project__task", "__get__team_project", "__get__task", "__create__task"
      ],
      "permission":"ALLOW"
    },
    {
      "accessType":"EXECUTE",
      "principalType":"ROLE",
      "principalId":"$owner",
      "property":[
        "__create__cuti", "__destroyById__cuti","__updateById__cuti",
        "__create__kehadiran", "__get__kehadiran",
        "__get__profil", "__create__profil", "__updateById__profil", "__find__profil", "__get__team_project", "__get__task"
      ],
      "permission":"ALLOW"
    }
  ],
  "methods": {}
}
